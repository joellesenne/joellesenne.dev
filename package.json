{
  "name": "joellesenne.dev",
  "version": "1.0.0",
  "private": true,
  "description": "My Web Site",
  "keywords": [
    "gatsby",
    "joellesenne",
    "portfolio",
    "react"
  ],
  "homepage": "https://joellesenne.dev",
  "bugs": {
    "url": "https://github.com/joellesenne/joellesenne.dev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joellesenne/joellesenne.dev.git"
  },
  "license": "MIT",
  "scripts": {
    "_comment": "[sudo] npm install -g next-update",
    "build": "gatsby build",
    "check": "npx npm-check-updates -u",
    "check:updates": "npx npm-check --skip-unused --update",
    "clean": "rm -rf .cache & rm -rf public",
    "cy:open": "cypress open",
    "dev": "gatsby develop -o",
    "develop": "gatsby develop",
    "format": "prettier \"**/*.{md,mdx} \" --write",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:ci": "eslint --format junit -o results/eslint/result.xml",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "next:update": "next-update -k true --tldr",
    "now-build": "npm run build",
    "serve": "gatsby serve",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect && yarn run snyk-protect",
    "sort:package": "npx sort-package-json",
    "test:cy": "start-server-and-test develop http://localhost:8000 cy:open"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "@mdx-js/tag": "^0.20.3",
    "babel-plugin-styled-components": "^1.10.7",
    "commentbox.io": "^2.0.0",
    "gatsby": "^2.23.11",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-catch-links": "^2.3.7",
    "gatsby-plugin-crisp-chat": "^3.2.2",
    "gatsby-plugin-feed": "^2.5.7",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-analytics": "^2.3.6",
    "gatsby-plugin-lodash": "^3.3.6",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-mdx": "^1.2.18",
    "gatsby-plugin-netlify": "^2.3.7",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-plugin-sitemap": "^2.4.7",
    "gatsby-plugin-styled-components": "^3.3.6",
    "gatsby-remark-autolink-headers": "^2.3.7",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.13",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "keyboardjs": "^2.6.3",
    "lodash": "^4.17.15",
    "mdx-utils": "0.2.0",
    "polished": "^3.6.5",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-cookie-consent": "^5.1.2",
    "react-dom": "^16.13.1",
    "react-dom-confetti": "^0.2.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.1.0",
    "react-live": "^2.2.2",
    "react-particles-js": "^3.2.1",
    "react-use": "^15.3.0",
    "snyk": "^1.351.0",
    "socialshares": "^2.0.5",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^4.9.0",
    "cypress-testing-library": "^4.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gatsby-plugin-root-import": "^2.0.5",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0"
  },
  "snyk": true
}
